{
  "name": "mozaicfm",
  "version": "0.1.2",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "setup": "git config --local core.hooksPath .githooks && corepack enable pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "fmt": "biome format --write . && prettier -w './**/*.{md,yml}'",
    "lint": "biome check . --write && eslint src/app --fix",
    "check": "pnpm fmt && pnpm lint",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy --branch main",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN",
    "build-style-dictionary": "style-dictionary build -c ./style-dictionary.config.mjs",
    "token-transform": "token-transformer designTokens/tokens.json designTokens/_transformedTokens.json GlobalToken,SemanticToken/light,SemanticToken/dark --expandTypography"
  },
  "dependencies": {
    "next": "14.2.13",
    "next-mdx-remote": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^2.0.2",
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241112.0",
    "@eslint/compat": "^1.2.2",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-links": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/nextjs": "^8.4.2",
    "@storybook/react": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@tokens-studio/sd-transforms": "^1.2.8",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "chromatic": "^11.18.1",
    "eslint": "^9.14.0",
    "eslint-config-next": "14.2.13",
    "eslint-plugin-storybook": "^0.8.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "storybook": "^8.4.2",
    "style-dictionary": "^4.2.0",
    "token-transformer": "^0.0.33",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vercel": "^37.14.0",
    "vitest": "^2.1.4",
    "wrangler": "^3.86.1"
  }
}
